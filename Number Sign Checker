#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a;
    scanf("%d",&a);
    if (a>0)
    printf("Positive");
    else if (a<0)
    printf("Negative");
    else
        printf("Zero");
        

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
