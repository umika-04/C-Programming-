#include<stdio.h>
#include<ctype.h>


int main() {
    char ch;
    scanf("%c",&ch);
    ch=toupper(ch);
    if (ch=='A' || ch== 'E' || ch== 'I' || ch== 'O' || ch == 'U')
        printf("Vowel");
    else if (!isalpha(ch))
            printf("Invalid Input");
    else 
        printf("Consonant");

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
